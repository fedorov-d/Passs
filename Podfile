source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/MacPass/KeePassKit'
platform :ios, '12.0'
inhibit_all_warnings!
use_frameworks!

# versions probably should be set explicitly
target 'Passs' do
    pod 'SnapKit'
    pod 'RealmSwift'
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RxRealm'
    pod 'KeePassKit'
    pod 'KissXML' -> 5.2.3
end
